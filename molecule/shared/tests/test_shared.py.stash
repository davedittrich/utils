# -*- coding: utf-8 -*-

import unittest

from molecule.shared import (
    host,
    testinfra_hosts,
)


class Test_Shared(unittest.TestCase):
    """Tests not specific to roles that should always be collected."""

    def setUp(self):
        self.testinfra_hosts = testinfra_hosts
        self.host = host

    def tearDown(self):
        pass

    @unittest.skipIf(True, reason='force skip')
    def test_shared_skip(self):
        self.assertTrue(False is True)


